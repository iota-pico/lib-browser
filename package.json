{
    "name": "@iota-pico/lib-browser",
    "version": "0.7.0",
    "description": "IOTA Pico Framework Library for Browser",
    "main": "pkg/iota-pico-lib-browser.js",
    "typings": "pkg/iota-pico-lib-browser.d.ts",
    "keywords": [
        "iota",
        "pico",
        "interface",
        "tangle",
        "javascript",
        "typescript",
        "API"
    ],
    "directories": {
        "doc": "docs",
        "test": "test"
    },
    "author": "martyn@iota.eco",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/iotaeco/iota-pico-lib-browser.git"
    },
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "build-clean": "rimraf ./dist/*",
        "build-compile": "tsc",
        "build": "npm-run-all build-clean build-compile",
        "pkg-clean": "rimraf ./pkg/*",
        "pkg-dev": "cross-env NODE_ENV=development webpack --progress",
        "pkg-prod": "cross-env NODE_ENV=production webpack --progress",
        "pkg-dts": "node ./dist/build-dts.js",
        "pkg-finalise": "rimraf ./pkg/bootstrap.js",
        "pkg": "npm-run-all pkg-clean pkg-dev pkg-prod pkg-dts pkg-finalise",
        "dist": "npm-run-all build pkg"
    },
    "dependencies": {
        "big-integer": "^1.6.26"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.40",
        "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
        "@babel/preset-env": "^7.0.0-beta.40",
        "@babel/runtime": "^7.0.0-beta.40",
        "@iota-pico/api": "^0.7.0",
        "@iota-pico/business": "^0.7.0",
        "@iota-pico/core": "^0.7.0",
        "@iota-pico/crypto": "^0.7.0",
        "@iota-pico/data": "^0.7.0",
        "@iota-pico/pal-browser": "^0.7.0",
        "@iota-pico/pow-box": "^0.7.0",
        "@iota-pico/pow-js": "^0.7.0",
        "@iota-pico/pow-wasm": "^0.7.0",
        "@iota-pico/pow-webgl": "^0.7.0",
        "@types/node": "^9.4.7",
        "babel-loader": "^8.0.0-beta.0",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "chai": "^4.1.2",
        "cross-env": "^5.1.3",
        "npm-run-all": "^4.1.2",
        "rimraf": "^2.6.2",
        "typescript": "^2.7.2",
        "webpack": "^4.1.1",
        "webpack-cli": "^2.0.10"
    }
}
